<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add xdt:Transform="Remove" xdt:Locator="Condition(@key = 'LocateApiAddressesUrl' and @value = '')"/>
    <add xdt:Transform="Remove" xdt:Locator="Condition(@key = 'LocateApiToken' and @value = '')"/>
  </appSettings>

  <appSettings xdt:Transform="Remove" xdt:Locator="Condition(count(*) = 0)" />
</configuration>